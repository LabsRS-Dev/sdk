{
  "name": "dove.max.sdk",
  "version": "1.0.1",
  "description": "common api for native macOS and electron",
  "main": "dist/dovemax-sdk.js",
  "module": "dist/dovemax-sdk.esm.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "types/index.d.ts",
    "types/helpers.d.ts"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/rollup.config.js",
    "build": "npm run build:main && npm run build:esm",
    "build:main": "rollup -c build/rollup.config.js && uglifyjs dist/dovemax-sdk.js -cm --comments -o dist/dovemax-sdk.min.js",
    "build:esm": "rollup -c build/rollup.config.js --environment ESM",
    "lint": "npm run lint:src",
    "lint:src": "eslint src",
    "lint:test": "eslint test",
    "test": "npm run lint && npm run test:sdk",
    "test:sdk": "jasmine JASMINE_CONFIG_PATH=test/sdk/jasmine.json",
    "release": "bash build/release.sh",
    "docs": "cd docs && gitbook serve",
    "docs:deploy": "cd docs && ./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabsRS-Dev/sdk.git"
  },
  "author": "Romanysoft LAB.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LabsRS-Dev/sdk/issues"
  },
  "homepage": "https://github.com/LabsRS-Dev/sdk#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "chromedriver": "^2.29.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.0.1",
    "express": "^4.15.2",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "nightwatch": "^0.9.14",
    "nightwatch-helpers": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^3.2.2",
    "selenium-server": "^2.53.1",
    "todomvc-app-css": "^2.1.0",
    "typescript": "^2.3.0",
    "uglify-js": "^2.8.22",
    "vue": "^2.2.6",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "rollup": "^0.41.4"
  }
}
